<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="Fondo" prefHeight="720.0" prefWidth="1280.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.cineuna.controller.PrincipalUsuariosViewController">
   <center>
      <VBox alignment="CENTER">
         <children>
            <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </center>
   <left>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="150.0" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="btnUsuario" minHeight="80.0" minWidth="200.0" onAction="#OnActionbtnUsuario" text="%usuarioTxt" VBox.vgrow="ALWAYS" />
            <JFXButton fx:id="btnComidas" minHeight="80.0" minWidth="200.0" onAction="#OnActionBtnComidas" text="%comidas" VBox.vgrow="ALWAYS" />
            <JFXButton fx:id="btnPeliculas" minHeight="80.0" minWidth="200.0" onAction="#OnActionbtnPeliculas" text="%pel" VBox.vgrow="ALWAYS" />
            <JFXButton fx:id="btnCerrar" minHeight="80.0" minWidth="200.0" onAction="#OnActionbtnCerrar" text="%cerrarSes" VBox.vgrow="ALWAYS" />
         </children></VBox>
   </left>
   <top>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="726.0" BorderPane.alignment="CENTER">
         <children>
            <Label styleClass="h1-contraste" text="%labelPrinCine" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Californian FB" size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
